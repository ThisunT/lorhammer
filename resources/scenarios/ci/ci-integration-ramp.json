[{
  "test": {
    "type": "ramp",
    "rampTime": "2m",
    "repeatTime": "0"
  },
  "stopAllLorhammerTime": "0",
  "sleepBeforeCheckTime": "2m",
  "shutdownAllLorhammerTime": "2m",
  "sleepAtEndTime": "0",
  "init": {
    "nsAddress": "127.0.0.1:1700",
    "nbGatewayPerLorhammer": 2,
    "nbNodePerGateway": [10, 10],
    "nbScenarioReplayLaps": 0,
    "scenarioSleepTime": ["10s", "10s"],
    "receiveTimeoutTime": "1s",
    "gatewaySleepTime": ["10ms", "10ms"]
  },
  "provisioning": {
    "type": "none"
  },
  "check": {
    "type": "prometheus",
    "config": {
      "address": "http://prometheus:9090",
      "checks": [
        {"query": "sum(lorhammer_long_request) + sum(lorhammer_durations_count)", "resultMin": 100, "resultMax": 10000, "description": "nb messages"},
        {"query": "count(lorhammer_durations_count)", "resultMin": 1, "resultMax": 1, "description": "nb lorhammer"},
        {"query": "sum(lorhammer_gateway)", "resultMin": 2, "resultMax": 2, "description": "nb gateways"},
        {"query": "sum(lorhammer_node)", "resultMin": 20, "resultMax": 20, "description": "nb nodes"}
      ]
    }
  },
  "deploy": {
    "type": "none"
  }
}]